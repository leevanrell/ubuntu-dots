#------------------------------
# Alias stuff
#------------------------------
alias ls="ls --color -F"
alias ll="ls --color -lh"
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'
alias diff='diff --color=auto'
alias ip='ip --color=auto'
alias pacman='pacman --color=auto'
alias gr="gvim --remote-silent"
alias vr="vim --remote-silent"
alias upz="source ~/.zshrc"
alias upb="source ~/.bashrc"
alias updp="xrandr --output DP-2-2 --off && xrandr --output DP-2-2 --auto && xrandr --output DP-2-2 --below HDMI-1"
alias cat="bat"
#alias py-mk="virtualenv --python=/usr/bin/python3.10 env && source env/bin/activate && pip3 install black nose nose2 coverage "
alias py-mk="python3 -m venv env && source env/bin/activate && pip3 install black nose nose2 coverage"
alias py-env="source env/bin/activate"
alias py-add="python -m ipykernel install --user --name=env"
#alias sshi= "ssh -i ~/shift5-govcloud-dev.pem"
alias aws-s5-setup="rsync -avzP \"ssh -i ~/.ssh/shift5-govcloud-dev.pem\" ~/aws-setup/*"
alias aws="aws --profile DeveloperAccess-204579938930"
alias dotfiles='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias kubectl="minikube kubectl --"
